<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="800.0" prefWidth="1300.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gra.startLocation">
    <children>
        <ImageView fitHeight="800.0" fitWidth="1300.0" pickOnBounds="true">
            <image>
                <Image url="@../../../../images/startLocation.jpg" />
            </image>
        </ImageView>
        <Label layoutX="110.0" layoutY="53.0" prefHeight="42.0" prefWidth="284.0" text="Jesteś w karczmie" textFill="WHITE">
            <font>
                <Font name="Candara Bold" size="33.0" />
            </font>
        </Label>
        <Button layoutX="38.0" layoutY="109.0" mnemonicParsing="false" onAction="#onTavernClick" prefHeight="65.0" prefWidth="214.0" text="Podejdź do karczmarza">
            <font>
                <Font name="Arial" size="16.0" />
            </font>
        </Button>
        <Button layoutX="278.0" layoutY="109.0" mnemonicParsing="false" onAction="#onGoToWhiteOrchardClick" prefHeight="65.0" prefWidth="184.0" text="Wyjdź">
            <font>
                <Font name="Arial" size="20.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
